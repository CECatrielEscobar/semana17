<div class="container-message" id="container-message">

</div>

<div class="formulario-register" id="formulario-register">
  <form action="" id="formulario">
    <label for="first_name" class="label-register">First name</label>
    <input
      type="text"
      name="first_name"
      id="first_name"
      placeholder="Enter your first name"
      class="input-register soyUnInput"
    />
    <label for="first_name" class="label-register">Last name</label>
    <input
      type="text"
      name="last_name"
      id="last_name"
      placeholder="Enter your last name"
      class="input-register soyUnInput"
    />
    <label for="email" class="label-register">Email</label>
    <input
      type="email"
      name="email"
      id="email"
      placeholder="Enter your Email"
      class="input-register soyUnInput"
    />
    <label for="password" class="label-register">Password</label>
    <input
      type="password"
      name="password"
      id="password"
      placeholder="Enter your password"
      class="input-register soyUnInput"
    />
    <label for="age" class="label-register">Age</label>
    <input
      type="number"
      name="age"
      id="age"
      placeholder="Enter your age"
      class="input-register soyUnInput"
    />

    <input
      type="submit"
      value="Registrar"
      class="input-register btn-register"
      id="input-submit"
    />
  </form>

</div>

<script src="/js/register.js"></script>